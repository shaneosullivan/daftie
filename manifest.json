{
  "name": "Dafty",
  "version": "1.0",
  "description": "Unoffical improvements for Daft.ie",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "storage",
    "https://www.google-analytics.com/collect*"
  ],
  "content_scripts": [
    {
      "matches": ["https://daft.ie/*", "https://www.daft.ie/*"],
      "css": ["index.css"],
      "js": ["analytics.js", "contentScript.js"]
    }
  ]
}
